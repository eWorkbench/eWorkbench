{
  "version": 1,
  "projects": {
    "eworkbench": {
      "projectType": "application",
      "root": "apps/eworkbench",
      "sourceRoot": "apps/eworkbench/src",
      "prefix": "eworkbench",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/eworkbench",
            "index": "apps/eworkbench/src/index.html",
            "main": "apps/eworkbench/src/main.ts",
            "polyfills": "apps/eworkbench/src/polyfills.ts",
            "tsConfig": "apps/eworkbench/tsconfig.app.json",
            "assets": [
              "apps/eworkbench/src/assets",
              {
                "glob": "**/*.*",
                "input": "apps/eworkbench/src/assets/plugins",
                "output": "/plugins/"
              },
              {
                "glob": "**/*.{js,css,woff}",
                "input": "node_modules/tinymce",
                "output": "/tinymce/"
              },
              {
                "glob": "**/*.*",
                "input": "node_modules/tinymce-formula",
                "output": "/tinymce/plugins/tinymce-formula/"
              },
              {
                "glob": "**/*.min.js",
                "input": "node_modules/@crawl/canvas-helper/dist",
                "output": "/canvas/"
              },
              {
                "glob": "**/*.{svg,eot,woff,ttf}",
                "input": "libs/icons/src/lib/components/icon/fonts",
                "output": "/fonts/"
              }
            ],
            "styles": ["apps/eworkbench/src/styles.scss"],
            "stylePreprocessorOptions": {
              "includePaths": ["apps/eworkbench/src/styles", "libs/forms/src"]
            },
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/eworkbench/src/environments/environment.ts",
                  "with": "apps/eworkbench/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all",
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "30kb",
                  "maximumError": "50kb"
                }
              ]
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "eworkbench:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "eworkbench:build:production"
            },
            "development": {
              "browserTarget": "eworkbench:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "eworkbench:build"
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["apps/eworkbench/src/**/*.ts"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/eworkbench/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "eworkbench-e2e": {
      "root": "apps/eworkbench-e2e",
      "sourceRoot": "apps/eworkbench-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/eworkbench-e2e/cypress.json",
            "tsConfig": "apps/eworkbench-e2e/tsconfig.e2e.json",
            "devServerTarget": "eworkbench:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "eworkbench:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-eslint/builder:lint",
          "options": {
            "lintFilePatterns": ["apps/eworkbench-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "forms": {
