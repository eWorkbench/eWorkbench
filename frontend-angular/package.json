{
  "name": "eworkbench",
  "version": "2.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main && patch-package",
    "nx": "nx",
    "akita": "akita",
    "start": "nx serve",
    "start:prod": "nx serve --prod",
    "build": "nx build",
    "build:stats": "nx build --stats-json",
    "build:prod": "nx build --prod && npm run i18n:optimize",
    "build:prod:stats": "nx build --prod --stats-json && npm run i18n:optimize",
    "test": "nx test",
    "test:all": "nx run-many --all --target=test",
    "test:c": "nx run-many --all --target=test --runInBand",
    "test:ci": "nx run-many --all --target=test --ci --runInBand",
    "test:update-snapshots": "nx run-many --all --target=test -u --runInBand",
    "lint": "nx workspace-lint && nx run-many --all --target=lint",
    "lint:fix": "nx workspace-lint && nx run-many --all --target=lint --fix",
    "prettier": "prettier --write apps/eworkbench/**/**/*.{js,jsx,ts,tsx,scss,css,less,md,html} libs/**/**/*.{js,jsx,ts,tsx,scss,css,less,md,html}",
    "i18n:extract": "transloco-keys-manager extract",
    "i18n:find": "transloco-keys-manager find",
    "i18n:optimize": "transloco-optimize dist/apps/eworkbench/assets/i18n",
    "e2e": "nx e2e",
    "e2e:ci": "nx e2e --headless",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "g:ws": "ng generate workspace-generator",
    "g:page": "nx workspace-generator pages --project=eworkbench",
    "g:m": "nx workspace-generator modules --project=eworkbench",
    "g:rm": "nx workspace-generator modules --project=eworkbench --routing=true",
    "g:c": "nx workspace-generator components --project=eworkbench",
    "g:directive": "nx workspace-generator directives --project=eworkbench",
    "g:guard": "nx workspace-generator guards --project=eworkbench",
    "g:i": "nx workspace-generator interceptors --project=eworkbench",
    "g:pipe": "nx workspace-generator pipes --project=eworkbench",
    "g:service": "nx workspace-generator services --project=eworkbench",
    "g:store": "nx workspace-generator stores --project=eworkbench",
    "g:library": "nx workspace-generator libraries"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "10.2.4",
    "@angular/cdk": "11.2.11",
    "@angular/common": "10.2.4",
    "@angular/compiler": "10.2.4",
    "@angular/core": "10.2.4",
    "@angular/forms": "10.2.4",
    "@angular/localize": "10.2.4",
    "@angular/platform-browser": "10.2.4",
    "@angular/platform-browser-dynamic": "10.2.4",
    "@angular/router": "10.2.4",
    "@crawl/angular-cropperjs": "2.0.0",
    "@crawl/canvas-helper": "0.2.2",
    "@datorama/akita": "6.1.3",
    "@datorama/akita-ng-entity-service": "6.0.0",
    "@fullcalendar/angular": "5.6.0",
    "@fullcalendar/daygrid": "5.6.0",
    "@fullcalendar/interaction": "5.6.0",
    "@fullcalendar/list": "5.6.0",
    "@fullcalendar/timegrid": "5.6.0",
    "@ng-select/ng-option-highlight": "0.0.6",
    "@ng-select/ng-select": "6.1.0",
    "@ngneat/dialog": "1.4.0",
    "@ngneat/error-tailor": "1.4.3",
    "@ngneat/reactive-forms": "1.7.2",
    "@ngneat/transloco": "2.20.1",
    "@ngneat/until-destroy": "8.0.4",
    "@ngx-loading-bar/core": "5.1.2",
    "@ngx-loading-bar/http-client": "5.1.2",
    "@ngx-loading-bar/router": "5.1.2",
    "@nrwl/angular": "11.5.2",
    "@tinymce/tinymce-angular": "4.2.2",
    "angular-gridster2": "11.1.5",
    "angular-resizable-element": "3.3.5",
    "bootstrap": "4.6.0",
    "cropperjs": "1.5.11",
    "d3": "6.7.0",
    "date-fns": "2.21.1",
    "filesize": "6.3.0",
    "flatpickr": "^4.6.9",
    "hammerjs": "2.0.8",
    "lodash-es": "4.17.21",
    "mime": "^2.5.2",
    "ngx-bootstrap": "6.2.0",
    "ngx-color-picker": "11.0.0",
    "ngx-matomo-v9": "0.3.0",
    "ngx-toastr": "13.2.1",
    "pdfjs-dist": "2.6.347",
    "rxjs": "6.6.7",
    "skeleton-screen-css": "1.0.1",
    "tinymce": "5.7.1",
    "tinymce-formula": "1.1.0",
    "tslib": "2.2.0",
    "utility-types": "3.10.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@anexia/eslint-config-marine": "1.1.1",
    "@angular-devkit/architect": "^0.1002.3",
    "@angular-devkit/build-angular": "0.1002.1",
    "@angular-devkit/core": "10.2.0",
    "@angular-devkit/schematics": "10.2.0",
    "@angular-devkit/schematics-cli": "0.1002.1",
    "@angular-eslint/builder": "1.2.0",
    "@angular-eslint/eslint-plugin": "1.2.0",
    "@angular-eslint/eslint-plugin-template": "1.2.0",
    "@angular-eslint/template-parser": "1.2.0",
    "@angular/cli": "10.2.1",
    "@angular/compiler-cli": "10.2.4",
    "@angular/language-service": "10.2.4",
    "@babel/core": "7.13.16",
    "@babel/plugin-transform-runtime": "7.13.15",
    "@babel/preset-env": "7.13.15",
    "@commitlint/cli": "12.1.1",
    "@datorama/akita-cli": "6.1.1",
    "@datorama/akita-ng-router-store": "6.0.0",
    "@datorama/akita-ngdevtools": "6.0.0",
    "@ngneat/spectator": "7.1.0",
    "@ngneat/transloco-keys-manager": "2.7.2",
    "@ngneat/transloco-optimize": "^1.0.2",
    "@nrwl/cli": "11.5.2",
    "@nrwl/cypress": "11.5.2",
    "@nrwl/eslint-plugin-nx": "11.5.2",
    "@nrwl/jest": "11.5.2",
    "@nrwl/tao": "11.5.2",
    "@nrwl/workspace": "11.5.2",
    "@schematics/angular": "10.2.1",
    "@types/chance": "1.1.1",
    "@types/d3": "6.3.0",
    "@types/jest": "26.0.23",
    "@types/lodash-es": "4.17.4",
    "@types/mime": "^2.0.3",
    "@types/node": "14.14.37",
    "@types/uuid": "8.3.0",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/eslint-plugin-tslint": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "babel-jest": "26.6.3",
    "chance": "1.1.7",
    "codelyzer": "6.0.1",
    "commitlint-config-jira": "1.5.1",
    "commitlint-plugin-jira-rules": "1.5.1",
    "cypress": "6.8.0",
    "dotenv": "8.2.0",
    "eslint": "7.25.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-cypress": "2.11.2",
    "eslint-plugin-prettier": "3.4.0",
    "fast-check": "2.14.0",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "jest-preset-angular": "8.4.0",
    "lint-staged": "10.5.4",
    "ng-packagr": "10.1.2",
    "patch-package": "6.4.7",
    "prettier": "2.2.1",
    "ts-jest": "26.5.5",
    "ts-node": "9.1.1",
    "tsickle": "0.39.1",
    "tslint": "6.1.3",
    "typescript": "4.0.3"
  },
  "akitaCli": {
    "customFolderName": "true",
    "template": "angular",
    "basePath": "./apps/eworkbench/src/app/stores/"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint:fix && lint-staged && npm run affected:test && npm run affected:e2e && npm run build:prod",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx,scss,css,less,md,html}": [
      "prettier --write"
    ]
  },
  "commitlint": {
    "extends": [
      "jira"
    ],
    "plugins": [
      "commitlint-plugin-jira-rules"
    ],
    "rules": {
      "jira-task-id-max-length": [
        2,
        "always",
        15
      ]
    }
  },
  "vsc-organize-imports": {
    "orderSpecifiers": true,
    "orderSpecifiersAsSingleLine": true,
    "baseUrl": "src",
    "emptyLinesAfterImports": 1,
    "emptyLinesBetweenFilledGroups": 1,
    "groups": [
      {
        "groups": [
          "global"
        ],
        "sortBy": "path",
        "emptyLines": false
      },
      {
        "groups": [
          "absolute"
        ],
        "sortBy": "path",
        "emptyLines": false
      },
      {
        "groups": [
          "relative"
        ],
        "sortBy": "path",
        "emptyLines": false
      },
      {
        "groups": [
          "globalDirect",
          "absoluteDirect",
          "relativeDirect"
        ],
        "sortBy": "path",
        "emptyLines": false
      }
    ]
  }
}
