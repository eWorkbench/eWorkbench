{
  "name": "eworkbench",
  "version": "2.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main && patch-package",
    "start": "nx serve",
    "start:prod": "nx serve --prod",
    "build": "nx build",
    "build:prod": "nx build --prod",
    "test": "nx run-many --all --target=test",
    "test:update-snapshots": "nx run-many --all --target=test -u --runInBand",
    "lint": "prettier --check . && nx run-many --all --target=lint",
    "format": "prettier --write . && nx run-many --all --target=lint --fix",
    "i18n:extract": "transloco-keys-manager extract",
    "i18n:find": "transloco-keys-manager find",
    "e2e:watch": "nx e2e eworkbench-e2e --watch",
    "e2e": "nx e2e eworkbench-e2e",
    "prepare": "is-ci || husky install"
  },
