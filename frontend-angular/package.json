{
  "name": "eworkbench",
  "version": "2.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main && patch-package",
    "nx": "nx",
    "akita": "akita",
    "start": "nx serve",
    "start:prod": "nx serve --prod",
    "build": "nx build",
    "build:stats": "nx build --stats-json",
    "build:prod": "nx build --prod && npm run i18n:optimize",
    "build:prod:stats": "nx build --prod --stats-json && npm run i18n:optimize",
    "test": "nx run-many --all --target=test --runInBand",
    "test:all": "nx run-many --all --target=test --runInBand",
    "test:c": "nx run-many --all --target=test --runInBand",
    "test:ci": "nx run-many --all --target=test --ci --runInBand",
    "test:update-snapshots": "nx run-many --all --target=test -u --runInBand",
    "lint": "nx workspace-lint && nx run-many --all --target=lint",
    "lint:fix": "nx workspace-lint && nx run-many --all --target=lint --fix",
    "prettier": "prettier --write apps/eworkbench/**/**/*.{js,jsx,ts,tsx,scss,css,less,md,html} libs/**/**/*.{js,jsx,ts,tsx,scss,css,less,md,html}",
    "i18n:extract": "transloco-keys-manager extract",
    "i18n:find": "transloco-keys-manager find",
    "i18n:optimize": "transloco-optimize dist/apps/eworkbench/assets/i18n",
    "e2e": "nx e2e",
    "e2e:ci": "nx e2e --headless",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "g:ws": "ng generate workspace-generator",
    "g:page": "nx workspace-generator pages --project=eworkbench",
    "g:m": "nx workspace-generator modules --project=eworkbench",
    "g:rm": "nx workspace-generator modules --project=eworkbench --routing=true",
    "g:c": "nx workspace-generator components --project=eworkbench",
    "g:directive": "nx workspace-generator directives --project=eworkbench",
    "g:guard": "nx workspace-generator guards --project=eworkbench",
    "g:i": "nx workspace-generator interceptors --project=eworkbench",
    "g:pipe": "nx workspace-generator pipes --project=eworkbench",
    "g:service": "nx workspace-generator services --project=eworkbench",
    "g:store": "nx workspace-generator stores --project=eworkbench",
    "g:library": "nx workspace-generator libraries",
    "prepare": "is-ci || husky install"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "12.2.5",
    "@angular/cdk": "12.2.5",
    "@angular/common": "12.2.5",
    "@angular/compiler": "12.2.5",
    "@angular/core": "12.2.5",
    "@angular/forms": "12.2.5",
    "@angular/localize": "12.2.5",
    "@angular/platform-browser": "12.2.5",
    "@angular/platform-browser-dynamic": "12.2.5",
    "@angular/router": "12.2.5",
    "@crawl/angular-cropperjs": "2.0.0",
    "@crawl/canvas-helper": "0.2.2",
    "@datorama/akita": "6.2.3",
    "@datorama/akita-ng-entity-service": "6.2.0",
    "@fullcalendar/angular": "5.9.0",
    "@fullcalendar/daygrid": "5.9.0",
    "@fullcalendar/interaction": "5.9.0",
    "@fullcalendar/list": "5.9.0",
    "@fullcalendar/timegrid": "5.9.0",
    "@ng-select/ng-option-highlight": "0.0.6",
    "@ng-select/ng-select": "7.2.0",
    "@ngneat/dialog": "1.4.0",
    "@ngneat/error-tailor": "1.4.3",
    "@ngneat/reactive-forms": "1.7.5",
    "@ngneat/transloco": "2.23.2",
    "@ngneat/until-destroy": "8.1.4",
    "@ngx-loading-bar/core": "5.1.2",
    "@ngx-loading-bar/http-client": "5.1.2",
    "@ngx-loading-bar/router": "5.1.2",
    "@nrwl/angular": "12.9.0",
    "@tinymce/tinymce-angular": "4.2.4",
    "angular-gridster2": "12.1.0",
    "angular-resizable-element": "3.4.0",
    "bootstrap": "4.6.0",
    "cropperjs": "1.5.12",
    "d3": "6.7.0",
    "date-fns": "2.23.0",
    "filesize": "8.0.3",
    "flatpickr": "4.6.9",
    "hammerjs": "2.0.8",
    "lodash": "4.17.21",
    "mime": "2.5.2",
    "ngx-bootstrap": "7.1.0",
    "ngx-color-picker": "11.0.0",
    "ngx-matomo-v9": "0.4.0",
    "ngx-textarea-autosize": "2.0.3",
    "ngx-toastr": "14.1.3",
    "pdfjs-dist": "2.6.347",
    "pretty-quick": "3.1.1",
    "rxjs": "7.3.0",
    "skeleton-screen-css": "1.0.1",
    "tinymce": "5.10.0",
    "tinymce-formula": "1.1.0",
    "tslib": "2.3.1",
    "utility-types": "3.10.0",
    "zone.js": "0.11.4"
  },
  "devDependencies": {
    "@anexia/eslint-config-marine": "2.1.0",
    "@angular-devkit/architect": "0.1202.5",
    "@angular-devkit/build-angular": "12.2.5",
    "@angular-devkit/core": "12.2.5",
    "@angular-devkit/schematics": "12.2.5",
    "@angular-devkit/schematics-cli": "12.2.5",
    "@angular-eslint/builder": "12.4.1",
    "@angular-eslint/eslint-plugin": "12.4.1",
    "@angular-eslint/eslint-plugin-template": "12.4.1",
    "@angular-eslint/template-parser": "12.4.1",
    "@angular/cli": "12.2.5",
    "@angular/compiler-cli": "12.2.5",
    "@angular/language-service": "12.2.5",
    "@babel/core": "7.15.5",
    "@babel/plugin-transform-runtime": "7.15.0",
    "@babel/preset-env": "7.15.6",
    "@commitlint/cli": "13.1.0",
    "@datorama/akita-cli": "6.1.1",
    "@datorama/akita-ng-router-store": "6.0.2",
    "@datorama/akita-ngdevtools": "6.0.1",
    "@ngneat/spectator": "8.0.4",
    "@ngneat/tailwind": "7.0.3",
    "@ngneat/transloco-keys-manager": "2.7.5",
    "@ngneat/transloco-optimize": "1.0.2",
    "@nrwl/cli": "12.9.0",
    "@nrwl/cypress": "12.9.0",
    "@nrwl/eslint-plugin-nx": "12.9.0",
    "@nrwl/jest": "12.9.0",
    "@nrwl/linter": "12.9.0",
    "@nrwl/tao": "12.9.0",
    "@nrwl/workspace": "12.9.0",
    "@schematics/angular": "12.2.5",
    "@types/chance": "1.1.3",
    "@types/d3": "6.7.1",
    "@types/jest": "27.0.1",
    "@types/lodash": "4.14.172",
    "@types/mime": "2.0.3",
    "@types/node": "16.9.1",
    "@types/uuid": "8.3.1",
    "@typescript-eslint/eslint-plugin": "4.31.1",
    "@typescript-eslint/eslint-plugin-tslint": "4.31.1",
    "@typescript-eslint/parser": "4.31.1",
    "autoprefixer": "10.3.4",
    "babel-jest": "27.2.0",
    "chance": "1.1.8",
    "codelyzer": "6.0.2",
    "commitlint-config-jira": "1.5.1",
    "commitlint-plugin-jira-rules": "1.5.1",
    "cypress": "8.4.0",
    "dotenv": "10.0.0",
    "eslint": "7.32.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-cypress": "2.12.1",
    "eslint-plugin-prettier": "4.0.0",
    "fast-check": "2.17.0",
    "husky": "7.0.2",
    "is-ci": "3.0.0",
    "jest": "27.2.0",
    "jest-preset-angular": "9.0.7",
    "ng-packagr": "12.2.1",
    "patch-package": "6.4.7",
    "postcss": "8.3.6",
    "prettier": "2.4.0",
    "tailwindcss": "2.2.15",
    "ts-jest": "27.0.5",
    "ts-node": "10.2.1",
    "tsickle": "0.43.0",
    "tslint": "6.1.3",
    "typescript": "4.3.5"
  },
  "akitaCli": {
    "customFolderName": "true",
    "template": "angular",
    "basePath": "./apps/eworkbench/src/app/stores/"
  },
  "commitlint": {
    "extends": [
      "jira"
    ],
    "plugins": [
      "commitlint-plugin-jira-rules"
    ],
    "rules": {
      "jira-task-id-max-length": [
        2,
        "always",
        15
      ]
    }
  },
  "vsc-organize-imports": {
    "orderSpecifiers": true,
    "orderSpecifiersAsSingleLine": true,
    "baseUrl": "src",
    "emptyLinesAfterImports": 1,
    "emptyLinesBetweenFilledGroups": 1,
    "groups": [
      {
        "groups": [
          "global"
        ],
        "sortBy": "path",
        "emptyLines": false
      },
      {
        "groups": [
          "absolute"
        ],
        "sortBy": "path",
        "emptyLines": false
      },
      {
        "groups": [
          "relative"
        ],
        "sortBy": "path",
        "emptyLines": false
      },
      {
        "groups": [
          "globalDirect",
          "absoluteDirect",
          "relativeDirect"
        ],
        "sortBy": "path",
        "emptyLines": false
      }
    ]
  }
}
