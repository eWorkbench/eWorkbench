{
  "root": true,
  "ignorePatterns": ["!**/*"],
  "extends": ["@anexia/eslint-config-marine/prettier/node", "@anexia/eslint-config-marine/prettier/angular"],
  "parserOptions": {
    "project": "./tsconfig.*?.json"
  },
  "rules": {
    "prettier/prettier": [2, {}, { "usePrettierrc": true }],
    "@typescript-eslint/no-unsafe-argument": 0
  }
}
